      "source": [
        "class Solution:\n",
        "    def busyStudent(self, startTime: List[int], endTime: List[int], queryTime: int) -> int:\n",
        "        ans=0\n",
        "        for i in range(len(startTime)):\n",
        "            if startTime[i]<=queryTime<=endTime[i]:\n",
        "                ans+=1\n",
        "        return ans"
      ]
    }
  ]
}